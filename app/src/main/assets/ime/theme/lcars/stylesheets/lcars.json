{
  "\$schema": "https://schemas.florisboard.org/snygg/v2/stylesheet",
  "@defines": {
    "--canvas": "#0B0D13",
    "--panel": "#5E6573",
    "--panel-muted": "#8C93A3",
    "--panel-deep": "#2B313A",
    "--text": "#C7CCD8",
    "--text-micro": "#F2F4F8",
    "--accent-command": "#FF6A4D",
    "--accent-active": "#5AC7E0",
    "--accent-active-2": "#2BB7D0",
    "--border": "#2B313A",
    "--border-pressed": "#5AC7E0",
    "--accent-blue-deep": "#1E6D89"
  },
  "window": {
    "background": "var(--canvas)",
    "foreground": "var(--text)"
  },
  "keyboard": {
    "background": "var(--panel-deep)",
    "foreground": "var(--text)"
  },
  "smartbar": {
    "background": "var(--panel-deep)",
    "foreground": "var(--text)"
  },
  "smartbar-shared-actions-row": {
    "background": "var(--panel-deep)"
  },
  "smartbar-extended-actions-row": {
    "background": "var(--panel-deep)"
  },
  "smartbar-candidates-row": {
    "background": "var(--panel-deep)"
  },
  "key": {
    "background": "var(--panel)",
    "foreground": "var(--text)",
    "shape": "rounded-corner(14dp,14dp,14dp,14dp)",
    "border-color": "var(--border)",
    "border-width": "1dp"
  },
  "key:pressed": {
    "background": "var(--panel-deep)",
    "border-color": "var(--border-pressed)",
    "border-width": "1dp"
  },
  "key:disabled": {
    "background": "var(--panel-muted)",
    "foreground": "var(--text-micro)"
  },
  "key-hint": {
    "foreground": "var(--text-micro)"
  },
  "key[code=10]": {
    "background": "var(--accent-command)",
    "foreground": "#0A0A0A"
  },
  "key[code=-233]": {
    "background": "var(--panel)",
    "foreground": "var(--accent-active)"
  },
  "key[code=32]": {
    "background": "var(--panel)",
    "border-color": "var(--border)",
    "border-width": "1dp"
  }
}
